{
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "db:start": "docker compose up -d",
    "db:stop": "docker compose down",
    "db:seed": "cd ./apps/server && npm run db:seed",
    "server:build": "turbo run build --filter=server",
    "server:run": "turbo run start:prod --filter=server"
  },
  "devDependencies": {
    "@turbo/gen": "^1.9.7",
    "eslint": "^7.32.0",
    "eslint-config-custom": "*",
    "turbo": "latest"
  },
  "name": "the-dentor",
  "packageManager": "npm@8.19.4",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "engines": {
    "node": "v18.15.0",
    "npm": "8.19.4"
  }
}